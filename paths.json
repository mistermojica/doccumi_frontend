{
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@app/*": ["./src/*", "./dist/*"],
      "@appold/*": ["./src/*"],
      "@store/*": ["./src/store/*"],
      "@components": ["./src/components/index"],
      "@modules/*": ["./src/modules/*"],
      "@pages/*": ["./src/pages/*"],
      "~": ["./node_modules"],
      "@mui/icons-material": ["./node_modules/@mui/icons-material/esm"],
      "@types/scheduler": ["./node_modules/scheduler/cjs"],
      "chart.js": ["./node_modules/chart.js"]
    }
  }
}
